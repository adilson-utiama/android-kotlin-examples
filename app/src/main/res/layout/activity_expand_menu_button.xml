<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".prototypes.expandmenu.ExpandMenuButtonActivity">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/exp_menu_ex_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_toggle_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_add_white" />

        <net.cachapa.expandablelayout.ExpandableLayout
            android:id="@+id/expandableMenuHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:el_duration="300"
            app:el_expanded="false"
            tools:el_expanded="true"
            app:el_parallax="0.5">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingHorizontal="12dp"
                    android:background="@color/colorAccent"
                    android:orientation="horizontal">

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_swap_vert"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_app_icon"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_play_arrow_gray"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_videocam_white"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_edit_white"
                        app:tint="@android:color/white" />

                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.cardview.widget.CardView>



        </net.cachapa.expandablelayout.ExpandableLayout>


    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        app:cardCornerRadius="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exp_menu_ex_1">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/colorAccentLight">

            <net.cachapa.expandablelayout.ExpandableLayout
                android:id="@+id/expandableMenuHorizontal2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:el_duration="300"
                app:el_expanded="false"
                app:el_parallax="0.5"
                tools:el_expanded="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingHorizontal="16dp">

                    <View
                        android:layout_width="56dp"
                        android:layout_height="36dp" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_swap_vert"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_app_icon"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_play_arrow_gray"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_videocam_white"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_edit_white"
                        app:tint="@android:color/white" />

                </androidx.appcompat.widget.LinearLayoutCompat>

            </net.cachapa.expandablelayout.ExpandableLayout>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btn_toggle_horizontal2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_add_white" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/cardView2"
        app:layout_constraintTop_toBottomOf="@+id/cardView2">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_toggle_horizontal3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_add_white" />

        <net.cachapa.expandablelayout.ExpandableLayout
            android:id="@+id/expandableMenuHorizontal3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:orientation="horizontal"
            app:el_duration="600"
            app:el_expanded="false"
            app:el_parallax="0.5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:el_expanded="true">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingHorizontal="16dp">

                <View
                    android:layout_width="24dp"
                    android:layout_height="24dp" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_mic_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_videocam_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_done_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_call_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_send_white" />

            </androidx.appcompat.widget.LinearLayoutCompat>

        </net.cachapa.expandablelayout.ExpandableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <net.cachapa.expandablelayout.ExpandableLayout
            android:id="@+id/expandableMenuVertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:el_duration="500"
            app:el_expanded="false"
            app:el_parallax="0.5"
            tools:el_expanded="true">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/colorAccent"
                    android:orientation="vertical"
                    android:paddingVertical="12dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_swap_vert"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_app_icon"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_play_arrow_gray"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_videocam_white"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_edit_white"
                        app:tint="@android:color/white" />

                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.cardview.widget.CardView>

        </net.cachapa.expandablelayout.ExpandableLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_toggle_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_add_white" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        app:cardCornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/linearLayoutCompat5">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/colorAccentDark">

            <net.cachapa.expandablelayout.ExpandableLayout
                android:id="@+id/expandableMenuVertical2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:el_duration="300"
                app:el_expanded="false"
                app:el_parallax="0.5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:el_expanded="true">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:paddingVertical="16dp"
                    android:orientation="vertical">



                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_swap_vert"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_app_icon"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_play_arrow_gray"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_videocam_white"
                        app:tint="@android:color/white" />

                    <androidx.appcompat.widget.AppCompatImageView
                        style="@style/AppTheme.ClickableViewBorderless"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        app:srcCompat="@drawable/ic_edit_white"
                        app:tint="@android:color/white" />

                    <View
                        android:layout_width="36dp"
                        android:layout_height="56dp" />

                </androidx.appcompat.widget.LinearLayoutCompat>

            </net.cachapa.expandablelayout.ExpandableLayout>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btn_toggle_vertical2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/ic_add_white" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cardView_vertical">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_toggle_vertical3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_add_white" />

        <net.cachapa.expandablelayout.ExpandableLayout
            android:id="@+id/expandableMenuVertical3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:el_duration="600"
            app:el_expanded="false"
            app:el_parallax="0.5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:el_expanded="true">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:paddingVertical="16dp">


                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_mic_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_videocam_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_done_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_call_white" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    app:fabSize="mini"
                    app:srcCompat="@drawable/ic_send_white" />

                <View
                    android:layout_width="24dp"
                    android:layout_height="50dp" />

            </androidx.appcompat.widget.LinearLayoutCompat>

        </net.cachapa.expandablelayout.ExpandableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>